<?xml version="1.0" encoding="utf-8"?>
<Report DataSourceName="jsonProductSalesData" Width="17cm" Name="ProductSales" Zoom="2" SnapGridSize="0.1cm" xmlns="http://schemas.telerik.com/reporting/2020/2.0">
  <DataSources>
    <JsonDataSource Name="jsonProductSalesData">
      <Source>
        <Uri Path="productSales.json" />
      </Source>
    </JsonDataSource>
  </DataSources>
  <Items>
    <PageHeaderSection Height="2cm" Name="pageHeaderSection1">
      <Style BackgroundColor="64, 64, 64" />
      <Items>
        <PictureBox Image="" Width="0.9cm" Height="1cm" Left="0.539cm" Top="0.5cm" Sizing="ScaleProportional" MimeType="image/png" Name="pictureBox2" />
        <TextBox Width="13.6cm" Height="0.9cm" Left="2.2cm" Top="0.739cm" Value="Telerik UI for Blazor + Telerik Reporting" Name="textBox15">
          <Style Color="255, 255, 255">
            <Font Name="Verdana" Size="16pt" />
          </Style>
        </TextBox>
        <TextBox Width="5.8cm" Height="0.4cm" Left="2.2cm" Top="0.4cm" Value="Sample Report" Name="textBox14">
          <Style Color="255, 255, 255">
            <Font Name="Verdana" />
          </Style>
        </TextBox>
      </Items>
    </PageHeaderSection>
    <DetailSection Height="0.2cm" Name="detailSection1">
      <Style Visible="False" />
    </DetailSection>
    <PageFooterSection Height="0.768cm" Name="pageFooterSection1">
      <Style BackgroundColor="64, 64, 64" />
      <Items>
        <TextBox Width="3.2cm" Height="0.568cm" Left="0.339cm" Top="0.086cm" Value="Sample Report" Name="textBox4">
          <Style Color="255, 255, 255">
            <Font Name="Verdana" />
          </Style>
        </TextBox>
        <TextBox Width="3.2cm" Height="0.486cm" Left="13.6cm" Top="0.086cm" Value="= &quot;PAGE: &quot; + PageNumber" Name="textBox5">
          <Style Color="255, 255, 255" TextAlign="Right">
            <Font Name="Verdana" />
          </Style>
        </TextBox>
      </Items>
    </PageFooterSection>
    <ReportHeaderSection Height="4.9cm" Name="reportHeaderSection1">
      <Style BackgroundColor="105, 105, 105" />
      <Items>
        <TextBox Width="9.7cm" Height="1.2cm" Left="1.3cm" Top="0.9cm" Value="Sales Report" Name="textBox1">
          <Style Color="255, 255, 255">
            <Font Name="Verdana" Size="22pt" Bold="False" />
          </Style>
        </TextBox>
        <TextBox Width="13.3cm" Height="1.8cm" Left="1.3cm" Top="2.1cm" Value="The graph is based on the filtering applied to the Grid, and shows top 10 products of the filtered results. If no grouping is applied, only one graph displaying the total will show. If grouping is applied, the graph will repeat for each group showing top 10 products." Name="textBox3">
          <Style Color="255, 255, 255">
            <Font Name="Verdana" Size="9pt" />
          </Style>
        </TextBox>
      </Items>
    </ReportHeaderSection>
  </Items>
  <PageSettings PaperKind="A4" Landscape="False">
    <Margins>
      <MarginsU Left="2cm" Right="2cm" Top="2cm" Bottom="2cm" />
    </Margins>
  </PageSettings>
  <StyleSheet>
    <StyleRule>
      <Style>
        <Padding Left="2pt" Right="2pt" />
      </Style>
      <Selectors>
        <TypeSelector Type="TextItemBase" />
        <TypeSelector Type="HtmlTextBox" />
      </Selectors>
    </StyleRule>
  </StyleSheet>
  <Groups>
    <Group Name="group">
      <GroupHeader>
        <GroupHeaderSection Height="17.7cm" Name="groupHeaderSection">
          <Style BackgroundColor="176, 196, 222" />
          <Items>
            <Graph DataSourceName="jsonProductSalesData" Width="15.9cm" Height="7.9cm" Left="0.5cm" Top="1.1cm" Name="graph1">
              <PlotAreaStyle LineWidth="0cm" LineColor="211, 211, 211" />
              <Axes>
                <GraphAxis Name="graphAxis2">
                  <MajorGridLineStyle LineWidth="1px" LineColor="211, 211, 211" />
                  <MinorGridLineStyle Visible="False" LineWidth="1px" LineColor="211, 211, 211" />
                  <Scale>
                    <CategoryScale />
                  </Scale>
                </GraphAxis>
                <GraphAxis Name="graphAxis1" LabelFormat="{0:# ##0$}">
                  <MajorGridLineStyle LineWidth="1px" LineColor="211, 211, 211" />
                  <MinorGridLineStyle Visible="False" LineWidth="1px" LineColor="211, 211, 211" />
                  <Scale>
                    <NumericalScale />
                  </Scale>
                </GraphAxis>
              </Axes>
              <CoordinateSystems>
                <CartesianCoordinateSystem XAxis="graphAxis2" YAxis="graphAxis1" Name="cartesianCoordinateSystem1" />
              </CoordinateSystems>
              <Legend>
                <Style LineWidth="0cm" LineColor="211, 211, 211" />
              </Legend>
              <Style>
                <Padding Left="10px" Right="10px" Top="10px" Bottom="10px" />
              </Style>
              <SeriesGroups>
                <GraphGroup Name="nameGroup">
                  <Groupings>
                    <Grouping Expression="= Fields.Name" />
                  </Groupings>
                  <Sortings>
                    <Sorting Expression="= Fields.Name" Direction="Asc" />
                  </Sortings>
                  <Filters>
                    <Filter Expression="= Fields.Revenue" Operator="TopN" Value="10" />
                  </Filters>
                </GraphGroup>
              </SeriesGroups>
              <CategoryGroups>
                <GraphGroup Name="monthSoldGroup">
                  <Groupings>
                    <Grouping Expression="= Fields.MonthSold" />
                  </Groupings>
                  <Sortings>
                    <Sorting Expression="= Fields.MonthSold" Direction="Asc" />
                  </Sortings>
                </GraphGroup>
              </CategoryGroups>
              <Series>
                <LineSeries CoordinateSystem="cartesianCoordinateSystem1" CategoryGroup="monthSoldGroup" SeriesGroup="nameGroup" Size="null" MarkerSize="5px" MarkerMinSize="5px" MarkerMaxSize="50px" Y="= Sum(Fields.Revenue)" DataPointLabel="= Sum(Fields.Revenue)" Name="lineSeries1">
                  <LineStyle LineWidth="1px" />
                  <DataPointStyle Visible="False" />
                  <DataPointLabelStyle Visible="False" />
                  <LegendItem Value="= Fields.Name" />
                  <MissingValuesLineStyle Visible="False" LineWidth="1px" />
                  <ToolTip Title="= Fields.Name" Text="= Sum(Fields.Revenue)" />
                </LineSeries>
              </Series>
              <Titles>
                <GraphTitle Text="Revenue" Position="TopCenter">
                  <Style LineWidth="0cm" LineColor="211, 211, 211" />
                </GraphTitle>
              </Titles>
              <Bindings>
                <Binding Path="DataSource" Expression="= ReportItem.DataObject" />
              </Bindings>
            </Graph>
            <TextBox Width="12.9cm" Height="0.6cm" Left="0.5cm" Top="0.5cm" Value="= IIf(Parameters.groupedBy.Value Like &quot;1&quot;, &quot;No grouping applied&quot;, Parameters.groupName.Value + &quot;: &quot; + Fields(Parameters.groupedBy.Value))" Name="textBox2">
              <Style>
                <Font Bold="True" />
              </Style>
            </TextBox>
            <Graph DataSourceName="jsonProductSalesData" Width="15.9cm" Height="7.9cm" Left="0.5cm" Top="9.3cm" Name="graph2">
              <PlotAreaStyle LineWidth="0cm" LineColor="211, 211, 211" />
              <Axes>
                <GraphAxis Name="graphAxis4">
                  <MajorGridLineStyle LineWidth="1px" LineColor="211, 211, 211" />
                  <MinorGridLineStyle Visible="False" LineWidth="1px" LineColor="211, 211, 211" />
                  <Scale>
                    <CategoryScale />
                  </Scale>
                </GraphAxis>
                <GraphAxis Name="graphAxis3">
                  <MajorGridLineStyle LineWidth="1px" LineColor="211, 211, 211" />
                  <MinorGridLineStyle Visible="False" LineWidth="1px" LineColor="211, 211, 211" />
                  <Scale>
                    <NumericalScale />
                  </Scale>
                </GraphAxis>
              </Axes>
              <CoordinateSystems>
                <CartesianCoordinateSystem XAxis="graphAxis4" YAxis="graphAxis3" Name="cartesianCoordinateSystem2" />
              </CoordinateSystems>
              <Legend>
                <Style LineWidth="0cm" LineColor="211, 211, 211" />
              </Legend>
              <Style>
                <Padding Left="10px" Right="10px" Top="10px" Bottom="10px" />
              </Style>
              <SeriesGroups>
                <GraphGroup Name="nameGroup1">
                  <Groupings>
                    <Grouping Expression="= Fields.Name" />
                  </Groupings>
                  <Sortings>
                    <Sorting Expression="= Fields.Name" Direction="Asc" />
                  </Sortings>
                  <Filters>
                    <Filter Expression="= Fields.Revenue" Operator="TopN" Value="= 10" />
                  </Filters>
                </GraphGroup>
              </SeriesGroups>
              <CategoryGroups>
                <GraphGroup Name="monthSoldGroup1">
                  <Groupings>
                    <Grouping Expression="= Fields.MonthSold" />
                  </Groupings>
                  <Sortings>
                    <Sorting Expression="= Fields.MonthSold" Direction="Asc" />
                  </Sortings>
                </GraphGroup>
              </CategoryGroups>
              <Series>
                <LineSeries CoordinateSystem="cartesianCoordinateSystem2" CategoryGroup="monthSoldGroup1" SeriesGroup="nameGroup1" Size="null" MarkerSize="5px" MarkerMinSize="5px" MarkerMaxSize="50px" Y="= Sum(Fields.TotalOrderQty)" DataPointLabel="= Sum(Fields.TotalOrderQty)" Name="lineSeries2">
                  <LineStyle LineWidth="1px" />
                  <DataPointStyle Visible="False" />
                  <DataPointLabelStyle Visible="False" />
                  <LegendItem Value="= Fields.Name" />
                  <MissingValuesLineStyle Visible="False" LineWidth="1px" />
                  <ToolTip Title="= Fields.Name" Text="= Sum(Fields.TotalOrderQty)" />
                </LineSeries>
              </Series>
              <Titles>
                <GraphTitle Text="Total Quantity Sold" Position="TopCenter">
                  <Style LineWidth="0cm" LineColor="211, 211, 211" />
                </GraphTitle>
              </Titles>
              <Bindings>
                <Binding Path="DataSource" Expression="= ReportItem.DataObject" />
              </Bindings>
            </Graph>
          </Items>
        </GroupHeaderSection>
      </GroupHeader>
      <GroupFooter>
        <GroupFooterSection Height="0.132cm" Name="groupFooterSection">
          <Style Visible="True" BackgroundColor="64, 64, 64" />
        </GroupFooterSection>
      </GroupFooter>
      <Groupings>
        <Grouping Expression="= Fields(Parameters.groupedBy.Value)" />
      </Groupings>
      <Sortings>
        <Sorting Expression="= Fields(Parameters.groupedBy.Value)" Direction="Asc" />
      </Sortings>
    </Group>
  </Groups>
  <ReportParameters>
    <ReportParameter Name="groupedBy">
      <Value>
        <String>1</String>
      </Value>
    </ReportParameter>
    <ReportParameter Name="groupName" AllowNull="True" />
  </ReportParameters>
</Report>