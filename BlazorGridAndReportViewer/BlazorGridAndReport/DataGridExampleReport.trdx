<?xml version="1.0" encoding="utf-8"?>
<Report DataSourceName="datagridJson" Width="17cm" Name="DataGridExampleReport" SnapGridSize="0.1cm" xmlns="http://schemas.telerik.com/reporting/2020/2.0">
  <DataSources>
    <JsonDataSource Name="datagridJson">
      <Source>
        <String>Ww0KICAgICAgICB7DQogICAgICAgICAgICANCiAgICAgICAgfQ0KICAgIF0=</String>
      </Source>
    </JsonDataSource>
  </DataSources>
  <Items>
    <PageHeaderSection Height="3.4cm" Name="pageHeaderSection1">
      <Style BackgroundColor="64, 64, 64" />
      <Items>
        <Panel Width="17cm" Height="0.7cm" Left="0cm" Top="2.692cm" Name="panel2">
          <Style BackgroundColor="135, 206, 250" />
          <Items>
            <TextBox Width="2.5cm" Height="0.5cm" Left="13.9cm" Top="0.108cm" Value="Started Selling" Name="textBox10" />
            <TextBox Width="2.3cm" Height="0.5cm" Left="7.6cm" Top="0.108cm" Value="Active Selling" Name="textBox8" />
            <TextBox Width="3.3cm" Height="0.5cm" Left="10.4cm" Top="0.108cm" Value="Category" Name="textBox7" />
            <TextBox Width="5.1cm" Height="0.5cm" Left="1cm" Top="0.108cm" Value="Name" Name="textBox6" />
            <TextBox Width="1.5cm" Height="0.5cm" Left="6.1cm" Top="0.108cm" Value="Cost" Name="textBox1" />
          </Items>
        </Panel>
        <TextBox Width="5.8cm" Height="0.5cm" Left="2.2cm" Top="0.5cm" Value="Sample Report" Name="textBox14">
          <Style Color="255, 255, 255">
            <Font Name="Verdana" />
          </Style>
        </TextBox>
        <TextBox Width="13.359cm" Height="0.9cm" Left="2.2cm" Top="0.9cm" Value="Telerik UI for Blazor + Telerik Reporting" Name="textBox15">
          <Style Color="255, 255, 255">
            <Font Name="Verdana" Size="16pt" />
          </Style>
        </TextBox>
        <PictureBox Image="" Width="0.9cm" Height="1.1cm" Left="0.6cm" Top="0.6cm" Sizing="ScaleProportional" MimeType="image/png" Name="pictureBox2" />
      </Items>
    </PageHeaderSection>
    <DetailSection CanShrink="True" Height="3.5cm" Name="detailSection1">
      <Items>
        <TextBox Width="5.1cm" Height="0.5cm" Left="1cm" Top="0.092cm" Value="= Fields.Name" Name="textBox2" />
        <TextBox Width="3.3cm" Height="0.5cm" Left="10.4cm" Top="0.092cm" Value="= Fields.ProductCategoryName" Name="textBox3" />
        <TextBox Width="1.9cm" Height="0.5cm" Left="7.6cm" Top="0.092cm" Value="= Fields.ActiveSelling" Name="textBox4" />
        <TextBox Width="2.8cm" Height="0.5cm" Left="13.9cm" Top="0.092cm" Value="= Fields.SellStartDate" Format="{0:d}" Culture="en-US" Name="textBox9">
          <Style TextAlign="Right" />
        </TextBox>
        <TextBox Width="0.9cm" Height="0.7cm" Left="0cm" Top="0cm" Value="" Name="textBox11">
          <Action>
            <ToggleVisibilityAction DisplayExpandedMark="False">
              <Targets>
                <Target ItemName="panel1" />
              </Targets>
            </ToggleVisibilityAction>
          </Action>
        </TextBox>
        <Panel Width="17cm" Height="2.8cm" Left="0cm" Top="0.7cm" Name="panel1">
          <Style Visible="False" BackgroundColor="70, 130, 180">
            <BorderStyle Bottom="Solid" />
            <BorderWidth Default="1pt" />
          </Style>
          <Items>
            <PictureBox Value="= Fields.ThumbNailPhoto" Width="4.5cm" Height="2.4cm" Left="0.2cm" Top="0.2cm" Sizing="ScaleProportional" MimeType="" Name="pictureBox1" />
            <TextBox Width="3.2cm" Height="0.5cm" Left="6.9cm" Top="0.4cm" Value="= IIf(Fields.Color Is Null, &quot;N/A&quot;, Fields.Color)" Name="textBox17">
              <ConditionalFormatting>
                <FormattingRule>
                  <Style Color="65, 65, 65" />
                  <Filters>
                    <Filter Expression="= Fields.Color Is Null" Operator="Equal" Value="= True" />
                  </Filters>
                </FormattingRule>
              </ConditionalFormatting>
            </TextBox>
            <TextBox Width="3.2cm" Height="0.5cm" Left="13.5cm" Top="1.1cm" Value="= Fields.ProductId" Format="{0}" Culture="en-US" Name="textBox18">
              <Style TextAlign="Right" />
            </TextBox>
            <TextBox Width="3.2cm" Height="0.5cm" Left="13.5cm" Top="0.4cm" Value="= Fields.SellStartDate" Format="{0:dd.MM.yyyy}" Name="textBox19" />
            <TextBox Width="3.2cm" Height="0.5cm" Left="6.9cm" Top="1.1cm" Value="= IIf(Fields.Size Is Null, &quot;N/A&quot;, Fields.Size)" Name="textBox20">
              <ConditionalFormatting>
                <FormattingRule>
                  <Style Color="65, 65, 65" />
                  <Filters>
                    <Filter Expression="= Fields.Size Is Null" Operator="Equal" Value="= True" />
                  </Filters>
                </FormattingRule>
              </ConditionalFormatting>
            </TextBox>
            <TextBox Width="3.2cm" Height="0.5cm" Left="6.9cm" Top="1.8cm" Value="= IIf(Fields.Weight Is Null, &quot;N/A&quot;, Fields.Weight + &quot;g&quot;)" Name="textBox21">
              <ConditionalFormatting>
                <FormattingRule>
                  <Style Color="65, 65, 65" />
                  <Filters>
                    <Filter Expression="= Fields.Weight Is Null" Operator="Equal" Value="= True" />
                  </Filters>
                </FormattingRule>
              </ConditionalFormatting>
            </TextBox>
            <TextBox Width="1.9cm" Height="0.5cm" Left="5cm" Top="0.4cm" Value="Color:" Name="textBox22">
              <Style TextAlign="Right" />
            </TextBox>
            <TextBox Width="1.9cm" Height="0.5cm" Left="5cm" Top="1.1cm" Value="Size:" Name="textBox23">
              <Style TextAlign="Right" />
            </TextBox>
            <TextBox Width="1.9cm" Height="0.5cm" Left="5cm" Top="1.8cm" Value="Weight:" Name="textBox24">
              <Style TextAlign="Right" />
            </TextBox>
            <TextBox Width="2.2cm" Height="0.5cm" Left="11.3cm" Top="0.4cm" Value="Sold from:" Name="textBox25">
              <Style TextAlign="Right" />
            </TextBox>
            <TextBox Width="1.6cm" Height="0.5cm" Left="11.9cm" Top="1.1cm" Value="ID:" Name="textBox26">
              <Style TextAlign="Right" />
            </TextBox>
          </Items>
          <ConditionalFormatting>
            <FormattingRule StopIfTrue="True">
              <Style BackgroundColor="234, 242, 255" />
              <Filters>
                <Filter Expression="= RowNumber()%2" Operator="Equal" Value="0" />
              </Filters>
            </FormattingRule>
            <FormattingRule>
              <Style BackgroundColor="198, 226, 255" />
              <Filters>
                <Filter Expression="= RowNumber()%2" Operator="Equal" Value="1" />
              </Filters>
            </FormattingRule>
          </ConditionalFormatting>
        </Panel>
        <TextBox Width="1.5cm" Height="0.5cm" Left="6.1cm" Top="0.092cm" Value="=Fields.StandardCost" Format="{0:C2}" Name="textBox5" />
      </Items>
      <ConditionalFormatting>
        <FormattingRule StopIfTrue="True">
          <Style BackgroundColor="234, 242, 255" />
          <Filters>
            <Filter Expression="= RowNumber()%2" Operator="Equal" Value="0" />
          </Filters>
        </FormattingRule>
        <FormattingRule>
          <Style BackgroundColor="198, 226, 255" />
          <Filters>
            <Filter Expression="= RowNumber()%2" Operator="Equal" Value="1" />
          </Filters>
        </FormattingRule>
      </ConditionalFormatting>
    </DetailSection>
    <PageFooterSection Height="0.8cm" Name="pageFooterSection1">
      <Style BackgroundColor="64, 64, 64" />
      <Items>
        <TextBox Width="3cm" Height="0.6cm" Left="13.7cm" Top="0.161cm" Value="= PageNumber + &quot; of &quot; + PageCount&#xD;&#xA;" Name="textBox16">
          <Style Color="255, 255, 255" TextAlign="Right">
            <Font Name="Verdana" Size="8pt" />
          </Style>
        </TextBox>
        <TextBox Width="3.3cm" Height="0.6cm" Left="0.2cm" Top="0.161cm" Value="= &quot;TOTAL ITEMS: &quot; + Count(Fields.Name)&#xD;&#xA;" Name="textBox27">
          <Style Color="255, 255, 255">
            <Font Name="Verdana" Size="8pt" />
          </Style>
        </TextBox>
      </Items>
    </PageFooterSection>
  </Items>
  <PageSettings PaperKind="A4" Landscape="False">
    <Margins>
      <MarginsU Left="2cm" Right="2cm" Top="2cm" Bottom="2cm" />
    </Margins>
  </PageSettings>
  <StyleSheet>
    <StyleRule>
      <Style>
        <Padding Left="2pt" Right="2pt" />
      </Style>
      <Selectors>
        <TypeSelector Type="TextItemBase" />
        <TypeSelector Type="HtmlTextBox" />
      </Selectors>
    </StyleRule>
  </StyleSheet>
  <Groups>
    <Group Name="group">
      <GroupHeader>
        <GroupHeaderSection Height="0.8cm" Name="groupHeaderSection">
          <Style BackgroundColor="70, 130, 180" />
          <Items>
            <TextBox Width="11.9cm" Height="0.611cm" Left="0.5cm" Top="0.189cm" Value="= Parameters.groupName.Value + &quot;: &quot; + Fields(Parameters.groupedBy.Value)" Name="textBox12">
              <Style Color="255, 255, 255">
                <Font Bold="True" />
              </Style>
            </TextBox>
            <TextBox Width="3cm" Height="0.6cm" Left="13.4cm" Top="0.2cm" Value="= &quot;Total products: &quot; + Count(Fields.Name)" Name="textBox13">
              <Style Color="255, 255, 255" />
            </TextBox>
          </Items>
          <ConditionalFormatting>
            <FormattingRule>
              <Style Visible="False" />
              <Filters>
                <Filter Expression="= Parameters.groupedBy.Value" Operator="Equal" Value="1" />
              </Filters>
            </FormattingRule>
          </ConditionalFormatting>
        </GroupHeaderSection>
      </GroupHeader>
      <GroupFooter>
        <GroupFooterSection Height="0.6cm" Name="groupFooterSection">
          <Style Visible="False" BackgroundColor="240, 248, 255" />
        </GroupFooterSection>
      </GroupFooter>
      <Groupings>
        <Grouping Expression="= Fields(Parameters.groupedBy.Value)" />
      </Groupings>
      <Sortings>
        <Sorting Expression="= Fields(Parameters.groupedBy.Value)" Direction="Asc" />
      </Sortings>
    </Group>
  </Groups>
  <ReportParameters>
    <ReportParameter Name="groupedBy" AllowNull="True" AutoRefresh="True">
      <Value>
        <String>1</String>
      </Value>
    </ReportParameter>
    <ReportParameter Name="groupName" AllowNull="True" />
  </ReportParameters>
</Report>